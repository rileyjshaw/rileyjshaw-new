    "gatsby-plugin-mdx": "^5.13.0",
    "gatsby-plugin-offline": "^6.13.0",
    "gatsby-plugin-page-creator": "^5.13.0",
    "gatsby-plugin-preload-fonts": "^4.13.0",
    "gatsby-plugin-sharp": "^5.13.0",
    "gatsby-plugin-sitemap": "^6.13.0",
    "gatsby-remark-autolink-headers": "^6.13.0",
    "gatsby-remark-copy-linked-files": "^6.13.0",
    "gatsby-remark-images": "^7.13.0",
    "gatsby-source-filesystem": "^5.13.0",
    "gatsby-transformer-json": "^5.13.0",
    "gatsby-transformer-remark": "^6.13.0",
    "gatsby-transformer-sharp": "^5.13.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-time-slicer": "^2.0.1",
    "refractor": "^4.8.1",
    "remark-gfm": "^3.0.1",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/register": "^7.23.7",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@size-limit/file": "^11.0.1",
    "@svgr/cli": "^6.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "babel-preset-gatsby": "^3.13.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "copyright-header": "^0.4.6",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.3.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "gatsby-plugin-postcss": "^6.13.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.32",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.3",
    "json-stable-stringify": "^1.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "request": "^2.88.2",
    "size-limit": "^11.0.1",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.0"
  },
  "keywords": [
    "rileyjshaw"
  ],
  "license": "GNU GPLv3",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "cm": "cz",
    "curate": "concurrently \"npm run develop-everywhere\" \"nodemon -w project-scraper/curation/server.mjs project-scraper/curation/server.mjs\"",
    "deploy": "npm run format && gatsby build && gh-pages -d public -b main",
    "dev": "npm run develop",
    "develop": "gatsby develop -o",
    "develop-everywhere": "gatsby develop -H 0.0.0.0 -o",
    "format": "prettier --write \"**/*.{js,jsx,mjs,json,md,mdx}\" && stylelint \"src/**/*.css\" --fix",
    "lint": "npm run format",
    "postbuild": "npm run size",
    "preload-fonts": "gatsby-preload-fonts",
    "prepare": "husky install",
    "scrape": "cd project-scraper && npm run start",
    "scraper": "npm run scrape",
    "serve": "gatsby serve",
    "size": "size-limit",
    "start": "npm run develop",
    "generate-icons": "svgr --config-file ./svgr.config.js --icon --svg-props fill=\"currentColor\" --title-prop --ext jsx --out-dir src/icons -- icons"
  },
  "size-limit": [
    {
      "limit": "0.5 MB",
      "path": "public/*.js"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rileyjshaw/rileyjshaw-new"
  },
  "bugs": {
    "url": "https://github.com/rileyjshaw/rileyjshaw-new/issues"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
